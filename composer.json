{
    "name": "robustprogram/rp-mapbox-field",
    "description": "An extension allows for the use of mapbox in your Bolt CMS.",
    "type": "bolt-extension",
    "keywords": [
        "mapbox",
        "robustprogram"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Robustprogram",
            "email": "Not Available"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Robustprogram\\RPMapboxField\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Robustprogram\\RPMapboxField\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Robustprogram\\RPMapboxField\\Extension"
    }
}
